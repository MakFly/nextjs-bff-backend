{
  "name": "laravel-nextjs-rbac",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "overrides": {
    "vite": "^7.3.1"
  },
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "prod": "turbo run build --force && turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "devDependencies": {
    "prettier": "^3.4.0",
    "shadcn": "^3.8.5",
    "turbo": "^2.3.0"
  },
  "packageManager": "bun@1.1.0",
  "dependencies": {
    "@tanstack/react-devtools": "^0.9.5",
    "@tanstack/react-router-devtools": "^1.160.0"
  }
}
